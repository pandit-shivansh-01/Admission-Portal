<div>
    <center>
        <h1>BTECH Registration Form </h1>
        <h1 style="color: orangered;">Do Register!</h1>
    </center>
</div>
<div style="background-color: #304146;">
    <h4 class="text-white text-center">In Case any query, Kindly Contact: 7974978812</h4>
</div>
    <div class="container-fluid">
        <div class="container">
            <div class="row">
                <div class="col-md-5">
                    <div class="carousel slide" data-bs-ride="carousel" id="a">

                        <div class="carousel-indicators">
                          <button type="button" data-bs-target="#a" data-bs-slide-to="0" class="active"></button>
                          <button type="button" data-bs-target="#a" data-bs-slide-to="1" ></button>
                          <button type="button" data-bs-target="#a" data-bs-slide-to="2" ></button>
                        </div>
                        
                        
                        
                        <div class="carousel-inner">
                            <div class="carousel-item active" data-bs-interval="2000">
                                <div class="banner-box">
                                    <img src="/images/register1.jpg" class="w-100" height="800px" alt="...">
                                </div>
                          </div>0
                      
                          <div class="carousel-item" data-bs-interval="2000">
                                <div class="banner-box">
                                   
                                    <img src="/images/register2.jpg" class="w-100" height="800px" alt="...">
                                </div>
                          </div>1
                      
                          <div class="carousel-item" data-bs-interval="2000">
                                <img src="/images/register3.jpg" class="w-100" height="800px" alt="...">
                            </div>
                      </div>2
                        </div>  
                            
                            
                        <button class="carousel-control-prev" type="button" data-bs-target="#a" data-bs-slide="prev">
                          <span class="carousel-control-prev-icon"></span>
                        </button>
                      
                        <button class="carousel-control-next" type="button" data-bs-target="#a" data-bs-slide="next">
                          <span class="carousel-control-next-icon"></span>
                        </button>
                      </div><!--carousel end-->
                <div class="col-md-7">
                    <form action="/btechinsert" method="post" enctype="multipart/form-data">

                        <div class="form-group">
                            <label>Full Name:</label>
                            <input class="form-control" type="text" name="name" value="<%= s %>" required
                                placeholder="Enter Your Full Name" />
                            <span class="Error"></span>
                        </div>
                        <br>
                        <div class="form-group">
                            <label>Email:</label>
                            <input class="form-control" type="email" name="email" value="<%= e %>" required
                                placeholder="Enter Your Email" />
                            <span class="Error"></span>
                        </div>
                        <br>
                        <div class="form-group">
                            <label>Enter Mobile Number:</label>
                            <input class="form-control" type="number" name="phone" required
                                placeholder="Enter Mobile Number" />
                                <span class="Error"></span>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Gender:</label><br />
                                <label><input type="radio" name="gender" required value="Male" checked /> Male</label>
                                <label><input type="radio" name="gender" required value="Female" /> Female</label>
                                <label><input type="radio" name="gender" required value="Other" /> Other</label>
                                <span class="Error"></span>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>D.O.B:</label>
                                <input class="form-control" type="date" name="date" required
                                    placeholder="Enter Birth Date" />
                                <span class="Error"></span>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Address:</label>
                                <input class="form-control" type="address" name="address" required placeholder="Address" />
                                <span class="Error"></span>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Select College:</label>
                                <select class="form-control" name="college">
                                    <option selected=""> Select College</option>
                                    <option>ITM</option>
                                    <option>MITS</option>
                                    <option>RJIT</option>
                                    <option>IPS</option>
                                </select>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Select Branch:</label>
                                <select class="form-control" name="course">
                                    <option selected=""> Select Course</option>
                                    <option>B.Tech</option>
                                    
                                </select>
                            </div>
                            <br>
                            <div class="form-group">
                                <label>Select Branch:</label>
                                <select class="form-control" name="branch">
                                    <option selected=""> Select Branch</option>
                                    <option>CSE</option>
                                    <option>IT</option>
                                    <option>EC</option>
                                    <option>ME</option>
                                    <option>CE</option>
                                </select>
                            </div>
                            <br>
    
                            <div class="form-group text-center">
                                <input style="background-color: #304146;" class="btn btn-primary btn-block" type="submit" value="Submit" />
                            </div>
                        </form>
                    </div>
                </div>
    
            </div>
        </div>
            <%- include('../footer.ejs')%>